<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Вертикальная линия -->
    <item  android:left="10dp" android:right="10dp" android:gravity="center">
        <shape android:shape="rectangle">
            <size android:width="30dp" android:height="5dp"/>
            <solid android:color="#FFFFFF"/> <!-- Цвет линии -->
            <corners android:radius="10dp"/>
        </shape>
    </item>

    <!-- Горизонтальная линия -->
    <item android:gravity="center"> <!-- Сдвиг вниз для горизонтальной линии -->
        <shape android:shape="rectangle">
            <size android:width="5dp" android:height="30dp"/>
            <solid android:color="#FFFFFF"/> <!-- Цвет линии -->
            <corners android:radius="10dp"/>
        </shape>
    </item>
</layer-list>
